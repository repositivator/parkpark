input[type=text] {
    width: 50%;
    padding: 12px 20px;
    margin: 8px 8px 8px 8px;
    box-sizing: border-box;
    border: 3px solid #ccc;
    -webkit-transition: 0.5s;
    transition: 0.5s;
    outline: none;
}

input[type=text]:focus {
    border: 3px solid #1BA4E7;
}

input[type=range] {
    width: 50%;
    padding: 12px 20px;
    margin: 8px 8px 8px 8px;
    box-sizing: border-box;
    border: 3px solid #ccc;
    -webkit-transition: 0.5s;
    transition: 0.5s;
    outline: none;
}

input[type=range]:focus {
    border: 3px solid #1BA4E7;
}

.modal-body{  max-height: 420px;
    overflow-y: auto;}