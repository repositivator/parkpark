function loginCheck(){
    if(document.loginForm.m_email.value===""){
      alert("이메일을 입력하세요!");
      document.loginForm.m_email.focus();
    }else if(document.loginForm.m_passwd.value===""){
      alert("비밀번호를 입력하세요!");    
      document.loginForm.m_passwd.focus();
    }else{
      document.loginForm.submit();
      return true;
    }
}
