function main(){
	history.back(-1);
}
window.onload = function(){
	document.getElementById("btnSubmit").onclick = inputCheck;
	
	function inputCheck(){
		if(reviewForm.e_content.value===""){
			alert("후기를 입력해주세요.");
			reviewForm.e_content.focus();
			return;
		}
		document.reviewForm.submit();
	}

	
}